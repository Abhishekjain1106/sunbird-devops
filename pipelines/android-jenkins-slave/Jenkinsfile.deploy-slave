pipeline {
    agent {
        label 'general-staging'
    }
    stages {
        stage('Deploy') {
            steps {
              step ([$class: 'CopyArtifact',
                projectName: 'Build/Android_Jenkins_Slave',
                filter: 'metadata.json']);

               sh 'METADATA_FILE=metadata.json ENV=dev ./pipelines/android-jenkins-slave/deploy.sh'

               archive includes: "metadata.json"
            }
        }
    }
}

