#!groovy

node('general-dev') {

    currentBuild.result = "SUCCESS"

    checkout scm

    try {

       stage('Pre-Build'){

         sh('./pipelines/sunbird-player/installDeps.sh')

       }
        stage('Deploy'){
            sh 'METADATA_FILE=./pipelines/opensaber-enc/metadata.json ARTIFACT_LABEL=bronze ENV=dev ./pipelines/opensaber-enc/deploy.sh'
        }
    }
    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
