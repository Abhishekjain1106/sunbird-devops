#!groovy

node('general-dev') {

    currentBuild.result = "SUCCESS"

    try {

       stage('Checkout'){

          checkout scm
       }

       stage('Pre-Build'){

         sh('./pipelines/sunbird-learner-service/installDeps.sh')

       }

       stage('Deploy'){
          sh ('./pipelines/dockerTagSilver.sh staging')
       }

    }
    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
