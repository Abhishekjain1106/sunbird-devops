#!groovy

node('general-spike') {

    currentBuild.result = "SUCCESS"

    try {

       stage('Checkout'){

          checkout scm
          sh('./pipelines/setupDevopsCode.sh')
       }

       stage('Pre-Build'){

         sh('./pipelines/sunbird-player/installDeps.sh')
        sh 'METADATA_FILE=metadata.json  ENV=spike ./pipelines/run-command/deploy.sh'
         }

       
    }
    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
