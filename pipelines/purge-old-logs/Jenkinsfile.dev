pipeline {
  agent {
      label 'general-dev'
  }
  triggers {
        cron('H 0 * * *')
  }
  stages {
    stage('Pre-Build'){
      steps {
        sh('./pipelines/purge-old-logs/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'ENV=dev ./pipelines/purge-old-logs/deploy.sh'
      }
    }
  }
}
