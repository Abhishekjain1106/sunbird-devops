pipeline {
  agent {
      label 'general-dev'
  }
  triggers {
        cron('H 0 * * *')
  }
  stages {
    stage('Checkout') {
      steps {
        sh('./pipelines/setupDevopsCode.sh')
      }
    }
    stage('Pre-Build'){
      steps {
        sh('./pipelines/es-backup/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'ENV=dev ./pipelines/es-backup/deploy.sh'
      }
    }
  }
}
