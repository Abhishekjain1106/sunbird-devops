pipeline {
  agent {
      label 'general-production'
  }
  triggers {
        cron('H 0 * * *')
  }
  stages {
    stage('Checkout') {
      steps {
        sh('./pipelines/setupDevopsCode.sh')
      }
    }
    stage('Pre-Build'){
      steps {
        sh('./pipelines/log-es-backup/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'ENV=ntp-production ./pipelines/log-es-backup/deploy.sh'
      }
    }
  }
}
