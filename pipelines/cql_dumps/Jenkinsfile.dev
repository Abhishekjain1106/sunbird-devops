#!groovy

node('build-slave') {

    currentBuild.result = "SUCCESS"

    try {
       stage('Checkout'){

          checkout scm
          sh('./pipelines/setupDevopsCode.sh')
       }

       stage('Pre-Build'){

         sh('./pipelines/sunbird-player/installDeps.sh')

       }

       stage('Deploy'){
          sh 'METADATA_FILE=metadata.json  ENV=dev ./pipelines/cql_dumps/deploy.sh'
       }

  
}

    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
