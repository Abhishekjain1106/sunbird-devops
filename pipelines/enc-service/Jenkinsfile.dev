#!groovy

node('general-dev') {

    currentBuild.result = "SUCCESS"

    try {

       stage('Checkout'){

          checkout scm
          sh" ./pipelines/setupDevopsCode.sh "
       }
        stage('Deploy'){
            sh 'METADATA_FILE=metadata.json ARTIFACT_LABEL=bronze ENV=dev ./pipelines/adminutils/deploy.sh'
        }
    }
    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
