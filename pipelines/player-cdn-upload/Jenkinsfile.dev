#!groovy

node('general-dev') {

    currentBuild.result = "SUCCESS"

    try {

       stage('Checkout'){

          checkout scm
          sh('./pipelines/player-cdn-upload/clone.sh')
       }

       stage('Pre-Build'){

         sh('./pipelines/player-cdn-upload/installDeps.sh')

       }

       stage('Deploy'){
          sh("./pipelines/player-cdn-upload/build.sh ${cdn}")
       }

    }
    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
