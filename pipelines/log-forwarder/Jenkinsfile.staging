pipeline {
  agent {
      label 'general-staging'
  }
  triggers {
        cron('H 0 * * *')
  }
  stages {
    stage('Checkout') {
      steps {
        sh('./pipelines/setupDevopsCodeOldStructure.sh')
      }
    }
    stage('Pre-Build'){
      steps {
        sh('./pipelines/log-forwarder/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'ENV=staging ./pipelines/log-forwarder/deployOldStructure.sh'
      }
    }
  }
}
