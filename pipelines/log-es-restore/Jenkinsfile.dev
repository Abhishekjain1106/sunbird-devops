pipeline {
  agent {
      label 'general-dev'
  }
  stages {
    stage('Pre-Build'){
      steps {
        sh('./pipelines/log-es-restore/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'ENV=dev SNAPSHOT_NUMBER=${snapshot_number} ./pipelines/log-es-restore/deploy.sh'
      }
    }
  }
}
