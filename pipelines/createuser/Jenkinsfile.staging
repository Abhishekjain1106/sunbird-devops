#!groovy

node('general-staging') {

    currentBuild.result = "SUCCESS"

    try {

       stage('Checkout'){

          checkout scm
          sh('./pipelines/setupDevopsCode.sh')
       }

       stage('Pre-Build'){

         sh('./pipelines/createuser/installDeps.sh')

       }

       stage('Deploy'){       
         sh '''
         ENV=dev ./pipelines/createuser/deploy.sh
         '''
       }
    }
    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
