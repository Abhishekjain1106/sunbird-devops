#!groovy

node('build-slave') {

    currentBuild.result = "SUCCESS"

    try {

       stage('Checkout'){

          checkout scm
          sh('./pipelines/setupDevopsCode.sh')
       }


       stage('Deploy'){       
         sh '''
         ENV=perf ./pipelines/createuser/deploy.sh
         '''
       }
    }
    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
