### Default variable file for player-service ###
  
namespace: {{ namespace }}
imagepullsecrets: {{ imagepullsecrets }}
dockerhub: {{ dockerhub }}

replicaCount: {{replicacount|default(1)}}
repository: {{player.repository|default('player')}}
image_tag: {{ player.image_tag }}
resources:
  cpu: {{player_service_cpu|default('50m')}}
  memory: {{player_service_memory|default('50Mi')}}
network:
  port: 3000
  targetport: 3000
strategy:
  type: RollingUpdate
  maxsurge: 1
  maxunavailable: 0

playerenv:
  sunbird_port: '"3000"'
  sunbird_content_player_url: {{sunbird_apimanager_service_base_url|d('""')}}
  sunbird_learner_player_url: {{sunbird_apimanager_service_base_url|d('""')}}
  sunbird_content_proxy_url: {{sunbird_content_service_url|d('""')}}
  sunbird_echo_api_url: {{sunbird_echo_api_url|d('""')}}
  sunbird_autocreate_trampoline_user: '"false"'
  sunbird_portal_realm: {{keycloak_realm|d('""')}}
  sunbird_portal_auth_server_url: {{keycloak_auth_server_url|d('""')}}
  sunbird_portal_auth_server_client: {{sunbird_portal_auth_server_client|d('""')}}
  sunbird_trampoline_client_id: {{sunbird_trampoline_client_id|d('""')}}
  sunbird_trampoline_secret: {{sunbird_trampoline_secret|d('""')}}
  ekstep_env: {{sunbird_env|d('""')}}
  sunbird_default_channel: {{sunbird_default_channel|d('""')}}
  sunbird_api_auth_token: {{sunbird_api_auth_token|d('""')}}
  sunbird_enable_permission_check: '"1"'
  sunbird_cassandra_urls: {{sunbird_cassandra_urls|d('""')}}
  sunbird_session_store_type: {{sunbird_session_store_type|d('""')}}
  AZURE_STORAGE_ACCOUNT: {{sunbird_azure_storage_account|d('""')}} 
  AZURE_STORAGE_KEY: {{sunbird_azure_storage_key|d('""')}}
  sunbird_dataservice_url: {{sunbird_dataservice_url|d('""')}}
  sunbird_telemetry_packet_size: '"{{sunbird_telemetry_packet_size|d('""')}}"'
  sunbird_environment: {{sunbird_environment|d('""')}}
  sunbird_instance: {{sunbird_instance|d('""')}}
  sunbird_android_app_url: {{sunbird_android_app_url|d('""')}}
  sunbird_external_content_whitelisted_domains: {{sunbird_external_content_whitelisted_domains|d('""')}}
  sunbird_portal_cdn_url: {{sunbird_portal_cdn_url|d('""')}}
  sunbird_portal_cdn_blob_url: {{sunbird_portal_cdn_blob_url|default('""')}}
  sunbird_portal_title_name: {{sunbird_portal_title_name|d('""')}}
  sunbird_enable_signup: '"{{sunbird_enable_signup|d('""')}}"'
  sunbird_tenant_cdn_url: {{sunbird_tenant_cdn_url|d('""')}}
  sunbird_cloud_storage_urls: {{sunbird_cloud_storage_urls|d('""')}}
  sunbird_config_service_url: {{sunbird_config_service_url|d('""')}}
  sunbird_portal_user_upload_ref_link: {{sunbird_portal_user_upload_ref_link|d('""')}}
  sunbird_build_number: {{sunbird_build_number|default('""')}}
  config_service_enabled: '"{{config_service_enabled|d('""')}}"'
  config_refresh_interval: '"{{config_refresh_interval|d('""')}}"'
  sunbird_device_register_api: {{sunbird_device_register_api|d('""')}}
  # Release-1.13
  sunbird_google_oauth_clientId: {{sunbird_google_oauth_clientId|d('""')}}
  sunbird_google_oauth_clientSecret: {{sunbird_google_oauth_clientSecret|d('""')}}
  sunbird_google_captcha_site_key: {{sunbird_google_captcha_site_key|d('""')}}
  sunbird_google_keycloak_client_id: {{sunbird_google_keycloak_client_id|d('""')}}
  sunbird_google_keycloak_secret: {{sunbird_google_keycloak_secret|d('""')}}
  sunbird_help_link_visibility: '"{{sunbird_help_link_visibility|default('FALSE')}}"'
  sunbird_azure_report_container_name: {{sunbird_azure_report_container_name|d('""')}}
  sunbird_azure_account_name: {{sunbird_azure_account_name|d('""')}}
  sunbird_azure_account_key: {{sunbird_azure_storage_key|d('""')}}
  sunbird_response_cache_ttl: '"{{sunbird_response_cache_ttl|default('180')}}"'
  sunbird_portal_preview_cdn_url: {{sunbird_portal_preview_cdn_url| d('""')}}
  {% if groups['cassandra-2'] is defined %}
sunbird_cassandra_consistency_level: quorum
  {% else %}
sunbird_cassandra_consistency_level: {{sunbird_cassandra_consistency_level|d('""')}}
  {% endif %}
sunbird_health_check_enable: '"{{sunbird_health_check_enable|d('""')}}"'
  sunbird_portal_player_cdn_enabled: {{sunbird_portal_player_cdn_enabled|default('""')}}
  # Release-2.0
  sunbird_processing_kafka_host: {{sunbird_processing_kafka_host|d('""')}}
  sunbird_sso_kafka_topic: {{sunbird_sso_kafka_topic|d('""')}}
  sunbird_portal_offline_tenant: {{sunbird_portal_offline_tenant|d('""')}}
  sunbird_portal_offline_supported_languages: {{sunbird_portal_offline_supported_languages|d('""')}}
  sunbird_portal_offline_app_release_date: {{sunbird_portal_offline_app_release_date|d('""')}}
  sunbird_portal_offline_app_version: {{sunbird_portal_offline_app_version|d('""')}}
  sunbird_portal_offline_app_download_url: {{sunbird_portal_offline_app_download_url|d('""')}}
  sunbird_portal_log_level: {{sunbird_portal_log_level|d('""')}}
  sunbird_google_android_keycloak_client_id: {{sunbird_google_android_keycloak_client_id|d('""')}}
  sunbird_google_android_keycloak_secret: {{sunbird_google_android_keycloak_secret|d('""')}}
  sunbird_trampoline_android_keycloak_client_id: {{sunbird_trampoline_android_keycloak_client_id|d('""')}}
  sunbird_trampoline_android_keycloak_secret: {{sunbird_trampoline_android_keycloak_secret|d('""')}}
  sunbird_android_keycloak_client_id: {{sunbird_android_keycloak_client_id|d('""')}}
  sunbird_portal_merge_auth_server_url: {{sunbird_subdomain_keycloak_base_url | d('""')}}
  
sunbird_cassandra_replication_strategy: '{{sunbird_cassandra_replication_strategy|d('""')}}'
