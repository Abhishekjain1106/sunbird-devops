ALERT KongClusterUnhealthy
IF kong_cluster_alive_nodes < {{ kong_cluster_expected_number_of_nodes }}
FOR 5m
LABELS {
  severity = "critical",
}
ANNOTATIONS {
  summary = "Kong cluster is unhealthy",
  description = "Number of live nodes : {% raw %}{{$value}}{% endraw %} is less than expected nodes : {{ kong_cluster_expected_number_of_nodes }}",
}
