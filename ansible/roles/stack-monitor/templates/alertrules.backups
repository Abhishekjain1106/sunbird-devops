ALERT stale_backup
  IF absent(azure_blob_latest_file_timestamp{job="data-backup-azure-blob-exporter"}) OR (time() * 1000) - azure_blob_latest_file_timestamp{job="data-backup-azure-blob-exporter"} > {{ expected_data_backup_interval_in_minutes|int * 60 * 1000 }}
  FOR 5m
  ANNOTATIONS {
      summary = "Backup is stale",
      description = "{% raw %}{{ $labels.container }}{% endraw %}: Latest backup file was created {% raw %}{{ $value / (60 * 1000) }}{% endraw %} minutes ago. Threshold: {{ expected_data_backup_interval_in_minutes }} minutes",
  }