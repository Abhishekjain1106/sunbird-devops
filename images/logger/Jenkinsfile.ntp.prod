#!groovy

node('general-production') {

    currentBuild.result = "SUCCESS"

    try {

       stage('Checkout'){

          checkout scm
       }

       stage('Pre-Build'){

         sh('./images/logger/installDeps.sh')

       }

       stage('Deploy'){
         sh 'ARTIFACT_LABEL=bronze ENV=ntp-production ./images/logger/deploy.sh'
       }
    }
    catch (err) {
        currentBuild.result = "FAILURE"
        throw err
    }

}
