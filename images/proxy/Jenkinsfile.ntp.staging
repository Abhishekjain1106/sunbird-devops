pipeline {
  agent {
      label 'general-staging'
  }
  stages {
    stage('Pre-Build'){
      steps {
        sh('./images/proxy/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh './images/proxy/metadata.sh > metadata.json'
        sh 'cat metadata.json'
        sh 'METADATA_FILE=./images/proxy/metadata.json ARTIFACT_LABEL=gold ENV=ntp-staging ./images/proxy/deploy.sh'
        archive includes: "images/proxy/metadata.json"
      }
    }
  }
}
